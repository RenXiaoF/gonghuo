/* 未审核 */
.logo_con{
  border-width: 0px;
  position: absolute;
  right:0;
  bottom: 80px;
  width: 70px;
  height: 70px;
}
.big_ellipse {
  border: 1px solid #fce4c5;
  position: absolute;
  left: 0px;
  top: 0px;
  width: 70px;
  height: 70px;
  border-radius: 50%;
}

.small_ellipse{
  border: 1px solid #fce4c5;
  position: absolute;
  left: 50%;
  top: 50%;
  width: 50px;
  height: 50px;
  margin:-25px 0 0 -25px;
  border-radius: 50%;
}

.text_box{
  text-align: center;
  border: 1px solid #fce4c5;
  position: absolute;
  left: -6%;
  top: 40%;
  width: 60px;
  padding:0 0 1px 20px;
  height: 18px;
  display: flex;
  -webkit-transform: rotate(320deg);
  -moz-transform: rotate(320deg);
  -ms-transform: rotate(320deg);
  transform: rotate(320deg);
  color:#fce4c5;
  background: #fff;
}
/* 已审核 */
  .logo_con_blue{
    border-width: 0px;
    position: absolute;
    right:0;
    bottom: 70px;
    width: 70px;
    height: 70px;
  }
  .logo_con_blue .big_ellipse {
    border: 1px solid #bae6f8;
  }
  
  .logo_con_blue .small_ellipse{
    border: 1px solid #bae6f8;
  }
  
  .logo_con_blue .text_box{
    border: 1px solid #bae6f8;
    color:#bae6f8;
  }
