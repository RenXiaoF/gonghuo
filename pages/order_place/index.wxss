/* tab切换 */
  .type-navbar {
    display: flex;
    /* background-color: #fff; */
    /* margin-top: 2vh; */
    /* border-bottom: 1rpx solid #e6e6e6; */
    /* padding-bottom: 1vh; */
    justify-content: center;
  }
  .type-box {
    width: 30%;
    box-sizing: border-box;
    font-size: 28rpx;
    line-height: 76rpx;
    padding: 0 20rpx;
    text-align: center;
    display: inline-block;
    overflow: hidden;
  }
  /* .type-navbar-item {
    border-bottom: 4rpx solid #fff;
  } */
  
  .type-item-on {
    font-size: 21 !important;
    font-weight: 700;
    color: #0486f4;
    position: relative;
  }
  .type-item-on::after {
    content: "";
    width: 60px;
    height: 3px;
    color: #0486f4 !important;
    background: #0486f4;
    border-radius: 2px;
    position: absolute;
    bottom: -2px;
    left: 50%;
    -webkit-transform: translateX(-50%);
    -ms-transform: translateX(-50%);
    transform: translateX(-50%);
  }

  .shop-info{
    display: flex;
    height: 15px;
    align-items: center;
    justify-content: space-between;
    font-size: 14px;
    background: #fff;
    margin: 10px 0;
    padding: 0 10px 10px;
  }

